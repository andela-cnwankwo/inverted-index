{
  "extends": "eslint-config-airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "require": true
  },
  "rules": {
    "one-var": 0,
    "no-tabs": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "class-methods-use-this": 0,
    "no-useless-escape": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-else-return": 0
  }
}